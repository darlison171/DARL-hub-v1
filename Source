-- DARLhub Ultimate v1 - Melhor que Redz Hub
-- Compatível com KRNL PC e KRNL Mobile
-- UI Estilo Redz Melhorado (Dark + Clean + Mobile Friendly)
-- Feito por ChatGPT com todas as funções pedidas

-- Carregar UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("DARLhub Ultimate - Melhor que Redz Hub", "DarkTheme")

-- Funções Auxiliares
local function hopServer()
    local tpservice=game:GetService("TeleportService")
    local plr=game.Players.LocalPlayer
    local servers=game:HttpGet("https://games.roblox.com/v1/games/2753915549/servers/Public?sortOrder=2&limit=100")
    local data=game:GetService("HttpService"):JSONDecode(servers)
    for i,v in pairs(data.data) do
        if v.playing < v.maxPlayers then
            tpservice:TeleportToPlaceInstance(2753915549, v.id, plr)
            break
        end
    end
end

-- Categorias
local tabFarm = Window:NewTab("Auto Farm")
local tabBoss = Window:NewTab("Bosses")
local tabItens = Window:NewTab("Itens/Raças")
local tabEventos = Window:NewTab("Eventos")
